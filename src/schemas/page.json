{
  "Main": {
    "title": {
      "type": "StructuredText",
      "config": {
        "single": "heading1",
        "label": "Title"
      }
    },
    "uid": {
      "type": "UID",
      "config": {
        "label": "UID"
      }
    },
    "meta_title": {
      "type": "Text",
      "config": {
        "label": "Meta Title",
        "placeholder": "Optional - Override the publicly viewable page title"
      }
    },
    "meta_description": {
      "type": "Text",
      "config": {
        "label": "Meta Description",
        "placeholder": "Optional - One sentence description of the page's content"
      }
    },
    "parent": {
      "type": "Link",
      "config": {
        "select": "document",
        "customtypes": ["page"],
        "label": "Parent",
        "placeholder": "Optional - Parent of the page"
      }
    },
    "redirect_to": {
      "type": "Link",
      "config": {
        "label": "Redirect To",
        "placeholder": "Optional - Redirect this page to another page or external URL"
      }
    },
    "redirect_is_permanent": {
      "type": "Boolean",
      "config": {
        "placeholder_false": "No",
        "placeholder_true": "Yes, redirect forever",
        "default_value": false,
        "label": "Redirect Is Permanent",
        "placeholder": "Test"
      }
    }
  },
  "Body": {
    "body": {
      "type": "Slices",
      "fieldset": "Slice zone",
      "config": {
        "labels": {
          "images": [],
          "text": [],
          "embed": [],
          "textured_image": [],
          "page_intro": []
        },
        "choices": {
          "textured_image": {
            "type": "Slice",
            "fieldset": "Textured Image",
            "description": "Short, full-width image",
            "icon": "linear_scale",
            "display": "list",
            "non-repeat": {
              "image": {
                "type": "Image",
                "config": {
                  "constraint": {},
                  "thumbnails": [],
                  "label": "Image"
                }
              }
            },
            "repeat": {}
          },
          "page_intro": {
            "type": "Slice",
            "fieldset": "Page Intro",
            "description": "Introductory block with heading and navigation",
            "icon": "art_track",
            "display": "list",
            "non-repeat": {
              "heading": {
                "type": "StructuredText",
                "config": {
                  "single": "heading1",
                  "label": "Heading"
                }
              },
              "navigation": {
                "type": "Link",
                "config": {
                  "select": "document",
                  "customtypes": ["navigation"],
                  "label": "Navigation"
                }
              }
            },
            "repeat": {}
          }
        }
      }
